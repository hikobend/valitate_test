CREATE TABLE `user` (
	`id` int  AUTO_INCREMENT PRIMARY KEY,
	`first` text not null,
	`last` text not null,
	`age` int not null,
	`email` text not null
);


CREATE TABLE `users` (
	`id` int  AUTO_INCREMENT PRIMARY KEY,
	`first` text not null,
	`last` text not null,
);

CREATE TABLE `date` (
	`id` int  AUTO_INCREMENT PRIMARY KEY,
	`date` date,
);